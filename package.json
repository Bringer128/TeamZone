{
  "name": "TeamZoneDemo",
  "version": "0.0.0",
  "description": "TeamZoneDemo",
  "main": "app.js",
  "author": "nickz",
  "dependencies": {
    "bcrypt": "*",
    "body-parser": "^1.10.0",
    "cookie-parser": "^1.3.3",
    "debug": "^2.1.0",
    "errno": "*",
    "errorhandler": "^1.3.0",
    "express": "4.x",
    "express-dependency-injector": "^2.0.3",
    "express-session": "^1.9.3",
    "express-validator": "^2.7.0",
    "jade": "*",
    "level-sublevel": "*",
    "levelup": "rvagg/node-levelup",
    "method-override": "^2.3.0",
    "middleware": "^1.0.0",
    "moment": "*",
    "morgan": "^1.5.0",
    "nodemailer": "^1.3.0",
    "redis": "*",
    "redisdown": "*",
    "serve-favicon": "^2.1.7",
    "string-format": "^0.2.1",
    "stylus": "*",
    "token": "*",
    "url": "*",
    "void-elements": "^1.0.0"
  },
  "devDependencies": {
    "mocha": "^1.20.1",
    "yadda": "0.10.14",
    "selenium-webdriver": "*",
    "async": "*",
    "blanket": "*",
    "jshint": "*",
    "level-cache": "*",
    "sinon": "*",
    "sinon-chai": "*",
    "mocha-sinon": "*",
    "chai": "*"
  },
  "scripts": {
    "test": "npm run test-unit && npm run test-unit-cov && npm run test-features",
    "test-unit": "mocha -r blanket -R spec ./lib/test/PlayerManagementServiceTests.js ./lib/test/UserManagementServiceLoginTests.js ./lib/test/UserManagementServiceConfirmRegisterTests.js ./lib/test/UserManagementServiceRegisterTests.js ./routes/test/userTests.js ./lib/test/MailGunEmailVerifyServiceTests.js",
    "test-unit-cov": "mocha -r blanket -R html-cov ./lib/test/PlayerManagementServiceTests.js ./lib/test/UserManagementServiceLoginTests.js ./lib/test/UserManagementServiceConfirmRegisterTests.js ./lib/test/UserManagementServiceRegisterTests.js ./routes/test/userTests.js ./lib/test/MailGunEmailVerifyServiceTests.js > Coverage.html",
    "test-features": "mocha -t 6000 -R spec ./feature-test/steps/testAddPlayer.js ./feature-test/steps/testLoginWithEmail.js ./feature-test/steps/testVerifyNewUserEmailAddress.js",
    "test-VerifyUserEmailAddress": "mocha -t 6000 -R spec ./feature-test/steps/testVerifyNewUserEmailAddress.js",
    "test-AddPlayer": "mocha -t 6000 -R spec ./feature-test/steps/testAddPlayer.js",
    "test-LoginWithEmail": "mocha -t 6000 -R spec ./feature-test/steps/testLoginWithEmail.js"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://teamzone.visualstudio.com/DefaultCollection/_git/TeamZone"
  },
  "license": "ISC"
}
